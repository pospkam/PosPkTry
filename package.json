{
  "name": "kamhub",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "db:setup": "bash scripts/setup-database.sh",
    "db:migrate": "npx ts-node scripts/run-migrations.ts",
    "sentry:sourcemaps": "sentry-cli sourcemaps upload --org kamhub --project nextjs .next",
    "postbuild": "npm run sentry:sourcemaps",
    "ai:update-knowledge": "node scripts/update-knowledge-base.js",
    "ai:update-knowledge:file": "node scripts/update-knowledge-base.js file"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.927.0",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "jose": "^5.2.0",
    "next": "14.2.15",
    "nodemailer": "^6.9.13",
    "pg": "^8.11.3",
    "react": "^18.3.1",
    "react-datepicker": "^8.9.0",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "tailwind-merge": "^3.4.0",
    "typescript": "5.4.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.11.25",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.11.2",
    "@types/react": "^18.2.64",
    "@types/react-datepicker": "^6.2.0",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.15",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.10",
    "vitest": "^1.3.1"
  }
}
