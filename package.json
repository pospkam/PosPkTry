{
  "name": "kamhub",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:unit": "vitest run --config vitest.config.ts",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:report": "playwright show-report",
    "test:all": "npm run test:unit && npm run test:e2e",
    "db:setup": "bash scripts/setup-database.sh",
    "db:migrate": "psql $DATABASE_URL -f scripts/apply-new-schemas.sql",
    "sentry:sourcemaps": "sentry-cli sourcemaps upload --org kamhub --project nextjs .next",
    "postbuild": "if [ -n \"$SENTRY_AUTH_TOKEN\" ]; then npm run sentry:sourcemaps; else echo 'Skipping Sentry sourcemap upload'; fi",
    "ai:update-knowledge": "node scripts/update-knowledge-base.js",
    "ai:update-knowledge:file": "node scripts/update-knowledge-base.js file"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.927.0",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.6.1",
    "jose": "^5.10.0",
    "next": "14.2.15",
    "nodemailer": "^6.9.13",
    "pg": "^8.11.3",
    "react": "^18.3.1",
    "react-datepicker": "^4.25.0",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "tailwind-merge": "^2.2.0",
    "typescript": "5.4.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.11.25",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.11.2",
    "@types/react": "^18.2.64",
    "@types/react-datepicker": "^4.19.0",
    "@types/react-dom": "^18.2.21",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.6.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.15",
    "jsdom": "^27.2.0",
    "playwright": "^1.56.1",
    "postcss": "^8.4.35",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.10",
    "vitest": "^1.6.1"
  }
}
